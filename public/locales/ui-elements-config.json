{
    "elements": [
        {
            "type": "text",
            "label": "Name",
            "placeholder": "Enter Name"
        },
        {
            "type": "text",
            "required": true,
            "id": "form-props-required",
            "label": "Required",
            "defaultValue": "Hello World"
        },
        {
            "type": "text",
            "disabled": true,
            "id": "form-props-disabled",
            "label": "Disabled",
            "defaultValue": "Hello World"
        },
        {
            "type": "password",
            "label": "Password",
            "id": "form-props-password-input",
            "autoComplete": "current-password"
        },
        {
            "type": "text",
            "label": "Read Only",
            "defaultValue": "Hello World",
            "id": "form-props-read-only-input",
            "InputProps": {
                "readOnly": true
            }
        },
        {
            "type": "number",
            "label": "Number",
            "id": "form-props-number",
            "InputLabelProps": {
                "shrink": true
            }
        },
        {
            "type": "text",
            "label": "Label",
            "placeholder": "Placeholder",
            "id": "form-props-full-width",
            "InputLabelProps": {
                "shrink": true
            }
        },
        {
            "type": "search",
            "id": "form-props-search",
            "label": "Search field"
        },
        {
            "type": "text",
            "label": "Helper text",
            "id": "form-props-helperText",
            "defaultValue": "Default Value",
            "helperText": "Some important text"
        }
    ]
}